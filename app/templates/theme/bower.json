{
  "name": "<%= themeNameSpace %>",
  "version": "0.1.0",
  "dependencies": {
    "bourbon": "^4.2",
    "neat": "^1.7",
    "modernizr": "~2.8.3",
    "wp-normalize.scss": "^1.0.0"
  },
  "ignore": [
    "node_modules",
    "components",
    "lib"
  ],
  "exportsOverride": {
    "bourbon": {
      "scss": "app/assets/stylesheets"
    },
    "neat": {
      "scss": "app/assets/stylesheets"
    },
    "modernizr": {
      "exclude": "modernizr.js"
    },
    "wp-normalize.scss": {
      "scss": "_wp-normalize.scss"
    }
  }
}
